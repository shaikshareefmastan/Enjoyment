pipeline {
   agent any
   stages {
      stage('git checkout') {
         steps {
            git 'https://github.com/shaikshareefmastan/Enjoyment.git'
         }
       }
      stage('Build application') {
         steps {
		 withDockerContainer(image: "selenium/standalone-chrome"){
           script{
                               sh "pwd"
                               sh "ls -la"
                               sh "mvn clean package -DskipTests"
			
                    }
                				
            }
            
         }
      }
   }
}
